#!/Users/engapa/Projects/BBVA/EuroCloud/git/modeldb-basic/.tox/py27/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'modeldb-basic','console_scripts','modeldb-config'
__requires__ = 'modeldb-basic'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('modeldb-basic', 'console_scripts', 'modeldb-config')()
    )
